<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/suche"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <de.kruemelopment.org.drckmichspiel.BackgroundView
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true">

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/button11"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="50dp"
        android:background="@drawable/eins" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/button13"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_alignTop="@+id/button11"
        android:layout_marginStart="20dp"
        android:layout_toEndOf="@+id/button11"
        android:background="@drawable/zwei" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/button16"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_alignBottom="@+id/button13"
        android:layout_marginStart="20dp"
        android:layout_toStartOf="@+id/button17"
        android:background="@drawable/drei" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/button17"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_alignBottom="@+id/button16"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="20dp"
        android:background="@drawable/vier" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/button18"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_alignStart="@+id/button11"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="50dp"
        android:background="@drawable/kreis" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/button20"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_alignStart="@+id/button13"
        android:layout_alignBottom="@+id/button18"
        android:background="@drawable/rechteck" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/button21"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_alignBottom="@+id/button20"
        android:layout_toStartOf="@+id/button17"
        android:background="@drawable/quadrat" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/button22"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_alignStart="@+id/button17"
        android:layout_alignBottom="@+id/button21"
        android:background="@drawable/smiley" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/imageView2"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignTop="@+id/button11"
        android:layout_alignEnd="@+id/button11"
        android:src="@drawable/hacken"
        app:srcCompat="@drawable/hacken" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/imageView3"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignTop="@+id/button18"
        android:layout_alignEnd="@+id/button18"
        android:src="@drawable/hacken"
        app:srcCompat="@drawable/hacken" />

    </RelativeLayout>

</RelativeLayout>